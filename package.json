{
  "name": "pillowfork",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "http://github.com/edrex/pillowfork.git"
  },
  "scripts": {
    "prepublish": "node ./node_modules/protractor/bin/webdriver-manager update",
    "postinstall": "node ./node_modules/bower/bin/bower install",
    "webdriver": "node ./node_modules/protractor/bin/webdriver-manager start",
